{
  "name": "con4gis/forum",
  "description": "Forum module for contao",
  "keywords": ["con4gis", "forum", "discussion", "panel", "board", "contao", "c4g", "forumbundle", "con4gisforum"],
  "type": "contao-bundle",
  "homepage": "https://con4gis.org",
  "license": "LGPL-3.0+",
  "authors":[
    {
      "name":"KÃ¼stenschmiede GmbH Software & Design",
      "homepage":"https://kuestenschmiede.de"
    }
  ],
  "require": {
    "con4gis/core": "~2.0"
  },
  "conflict": {
    "contao/core": "*",
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "support": {
    "issues": "https://github.com/Kuestenschmiede/ForumBundle/issues"
  },
  "autoload": {
    "psr-4": {
      "con4gis\\ForumBundle\\": ""
    },
    "classmap": [
      "Resources/contao/"
    ],
    "exclude-from-classmap": [
      "Resources/contao/config/",
      "Resources/contao/dca/",
      "Resources/contao/languages/",
      "Resources/contao/templates/"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "contao-manager-plugin": "con4gis\\ForumBundle\\ContaoManager\\Plugin"
  }
}
